{
    "name": "Distributed Tensorflow",
    "description": "Launch a Distributed Tensorflow Cluster than runs between-graph replicated training and evaluation.",
    "author": "sfox",
    "licensed": true,
    "classifications": [
        "Machine Learning/Deep Learning",
        "IBM POWER/Machine Learning",
        "IBM POWER/Deep Learning"
    ],
    "machines": [
        "np8g4"
    ],
    "vault-types": [
        "BLOCK",
        "FILE"
    ],
    "commands": {
        "demo": {
            "path": "/bin/bash",
            "interactive": false,
            "name": "Demo",
            "description": "Train a simple DNNRegressor Estimator on the Iris test data set",
            "parameters": {
                "-l": {
                    "name": "-l",
                    "description": "-l",
                    "type": "CONST",
                    "value": "-l",
                    "positional": true,
                    "required": true
                },
                "-c": {
                    "name": "-c",
                    "description": "-c",
                    "type": "CONST",
                    "value": "-c",
                    "positional": true,
                    "required": true
                },
                "command": {
                    "name": "Command",
                    "description": "Command to run",
                    "type": "CONST",
                    "value": "/usr/local/distributed-tensorflow/scripts/start_mpi.sh",
                    "positional": true,
                    "required": true
                },
                "script": {
                    "name": "Script",
                    "description": "Replicated device training script",
                    "type": "CONST",
                    "value": "/usr/local/distributed-tensorflow/scripts/simple_dist_experiment.py",
                    "positional": true,
                    "required": true
                },
                "EXPERIMENT_ID": {
                    "name": "Experiment Id",
                    "description": "unique ID of experiment, or previous job name to re-start",
                    "type": "STR",
                    "positional": false,
                    "variable": true,
                    "required": false
                }
            }
        },
        "distributed-tf": {
            "path": "/bin/bash",
            "interactive": false,
            "name": "Distributed Tensorflow",
            "description": "Run any distributed Tensorflow workflow with ps, master and worker processes.",
            "parameters": {
                "-l": {
                    "name": "-l",
                    "description": "-l",
                    "type": "CONST",
                    "value": "-l",
                    "positional": true,
                    "required": true
                },
                "-c": {
                    "name": "-c",
                    "description": "-c",
                    "type": "CONST",
                    "value": "-c",
                    "positional": true,
                    "required": true
                },
                "command": {
                    "name": "Command",
                    "description": "Command to run",
                    "type": "CONST",
                    "value": "/usr/local/distributed-tensorflow/scripts/start_mpi.sh",
                    "positional": true,
                    "required": true
                },
                "script": {
                    "name": "Script",
                    "description": "Replicated device training script",
                    "type": "FILE",
                    "filter": "*.py",
                    "positional": true,
                    "required": true
                },
                "EXPERIMENT_ID": {
                    "name": "Experiment Id",
                    "description": "unique ID of experiment, or previous job name to re-start",
                    "type": "STR",
                    "positional": false,
                    "variable": true,
                    "required": false
                }
            }
        },
        "developer-server": {
            "path": "/sbin/init",
            "interactive": true,
            "name": "Developer Server",
            "description": "Launch a server session with SSH to test Tensorflow-capable python code.",
            "parameters": {}
        },
        "tensorboard-only": {
            "path": "/bin/bash",
            "interactive": true,
            "name": "tensorboard",
            "description": "Launch tensorboard",
            "machines": [
                "np8c0"
            ],
            "parameters": {
                "-l": {
                    "name": "-l",
                    "description": "-l",
                    "type": "CONST",
                    "value": "-l",
                    "positional": true,
                    "required": true
                },
                "-c": {
                    "name": "-c",
                    "description": "-c",
                    "type": "CONST",
                    "value": "-c",
                    "positional": true,
                    "required": true
                },
                "command": {
                    "name": "Command",
                    "description": "Command to run",
                    "type": "CONST",
                    "value": "tensorboard",
                    "positional": true,
                    "required": true
                },
                "logdiropt": {
                    "name": "--logdir",
                    "description": "logdir",
                    "type": "CONST",
                    "value": "--logdir",
                    "positional": true,
                    "required": true
                },
                "logdir": {
                    "name": "--logdir",
                    "description": "logdir",
                    "type": "CONST",
                    "value": "/data/tensorflow-output",
                    "positional": true,
                    "required": true
                }
            }
        }
    },
    "image": {
        "data": "",
        "type": "image/png"
    }
}
